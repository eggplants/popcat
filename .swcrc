{
  "jsc": {
    "baseUrl": ".",
    "parser": {
      "decorators": true,
      "decoratorsBeforeExport": false,
      "syntax": "typescript",
      "tsx": false
    },
    "paths": {
      "~/*": ["src/*"]
    },
    "target": "es2020",
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    }
  },
  "module": {
    "type": "commonjs"
  }
}
